{
  "expo": {
    "name": "FlashlearnAI",
    "slug": "FlashlearnAI",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/main-icon (3).png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/main-icon (1).png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.FlashlearnAI"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/main-icon (2).png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "c995a94d-faa8-43f3-ba68-9aa74af8d8e5"
      }
    }
  }
}
